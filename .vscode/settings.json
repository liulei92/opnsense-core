{
  /* 是否添加兼容前缀 如： -webkit- , -moz- ... */
  "liveSassCompile.settings.autoprefix": [
    "> 1%",
    "last 2 versions"
  ],
  /* 排除目录 */
  "liveSassCompile.settings.excludeList": [
    "/**/node_modules/**",
    "/.vscode/**" 
  ],
  /* 包含目录 */
  "liveSassCompile.settings.includeItems": [
    "/src/opnsense/www/themes/grandstream/assets/stylesheets/main.scss"
  ],
  /* 是否生成对应的map */
  "liveSassCompile.settings.generateMap": false,
  /* 压缩设置 */
  "liveSassCompile.settings.formats":[
    // This is Default
    {
      "format": "compressed", // nested 嵌套, expanded, compact 紧凑, compressed 压缩
      "extensionName": ".css",
      "savePath": "~/../../build/css" // 为 null 表示当前目录
    }
  ],

  "less.compile": {
    "compress": false, // 是否压缩
    "sourceMap": false, // 是否生成map文件，设置为"true"时可以在调试台看到less行数
    "out": "../../build/css/", // 是否输出css文件，false为不输出
    "outExt": ".css", //输出文件的后缀，小程序可以写为"wxss"
  }
}